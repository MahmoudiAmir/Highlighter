{
    "manifest_version": 2,
    "name": "Highlighter Extension",
    "version": "1.0",
    "description": "A simple text highlighter extension",
    "icons": {
      "48": "images/iconEx.png"
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "browser_action": {
    
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }